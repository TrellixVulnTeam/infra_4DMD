{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Distribution build information",
  "title": "Golang project distribution info",
  "type": "object",
  "properties": {
    "artefact": {
      "description": "Artefact name",
      "type": "string"
    },
    "project": {
      "description": "ID of a project",
      "type": "string"
    },
    "product": {
      "description": "Product name",
      "type": "string"
    },
    "distribution": {
      "description": "Distribution name",
      "type": "string"
    },
    "repository": {
      "description": "ID of a repository",
      "type": "string"
    },
    "clone_url": {
      "description": "Repository clone url",
      "type": "string"
    },
    "builds": {
      "description": "List of build names",
      "type": "array",
      "items": {
        "description": "Build name",
        "type": "string"
      }
    }
  },
  "required": [
    "project",
    "product",
    "distribution",
    "repository",
    "clone_url",
    "builds"
  ]
}
